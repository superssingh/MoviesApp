/userContext\";\nimport MovieContext from \"../js/contexts/movieContext\";\n\nconst Home = () => {\n  const { currentUser } = useContext(UserContext);\n  const { movies, addMovies } = useContext(MovieContext);\n\n  useEffect(() => {\n    const abortController = new AbortController();\n\n    async function fetchData({ signal }) {\n      // You can await here\n      const { data } = await axios.get(TagNames.getURL());\n      addMovies(data.results);\n    }\n    fetchData(abortController.signal);\n    return function cleanup() {\n      abortController.abort();\n    };\n  }, []); // Or [] if effect doesn't need props or state\n\n  if (movies.length === 0) {\n    return <h3>Movies not found.</h3>;\n  }\n\n  return (\n    <div className=\"container\">\n      <div>Username: {currentUser.name}</div>\n      <div className=\"home\">\n        <MoviesList movies={movies} count={movies.length} />\n      </div>\n    </div>\n  );\n};\n\nexport default Home;\n","/Users/santoshsingh/Documents/WebDevelopment/ReactJS/MoviesApp/src/components/pages/contact.jsx",[],"/Users/santoshsingh/Documents/WebDevelopment/ReactJS/MoviesApp/src/components/pages/services.jsx",[],"/Users/santoshsingh/Documents/WebDevelopment/ReactJS/MoviesApp/src/components/pages/about.jsx",[],"/Users/santoshsingh/Documents/WebDevelopment/ReactJS/MoviesApp/src/components/pages/blog.jsx",[],"/Users/santoshsingh/Documents/WebDevelopment/ReactJS/MoviesApp/src/components/pages/shop.jsx",[],"/Users/santoshsingh/Documents/WebDevelopment/ReactJS/MoviesApp/src/components/pages/loginForm.jsx",[],"/Users/santoshsingh/Documents/WebDevelopment/ReactJS/MoviesApp/src/components/pages/not-found.jsx",[],"/Users/santoshsingh/Documents/WebDevelopment/ReactJS/MoviesApp/src/components/pages/movieDetails.jsx",["101"],"import React, { useState, useContext, useEffect } from \"react\";\nimport MovieContext from \"../js/contexts/movieContext\";\nimport * as TagNames from \"../js/constants/constants\";\nimport axios from \"axios\";\nimport { useHistory } from \"react-router-dom\";\n\nconst MovieDetails = (props) => {\n  const history = useHistory();\n  const { movies } = useContext(MovieContext);\n  const id = props.match.params.id;\n  const [movie, setMovie] = useState([]);\n  const [videoLinK, setVideoLink] = useState(\"\");\n  const [videoStatus, setVideoStatus] = useState(false);\n\n  useEffect(() => {\n    const abortController = new AbortController();\n    async function fetchData({ signal }) {\n      // You can await here\n      if (movies) {\n        try {\n          const selectedMovie = await movies.filter(\n            (m) => m.id === parseInt(id)\n          );\n          if (selectedMovie[0]) {\n            setMovie(selectedMovie[0]);\n            handleVideo(id);\n          } else {\n            return history.replace(\"/\");\n          }\n        } catch (ex) {\n          console.log(\"Error: \", ex);\n        }\n      }\n    }\n    if (id) {\n      fetchData(id, abortController.signal);\n    }\n    return function cleanup() {\n      abortController.abort();\n    };\n  }, []);\n\n  const handleVideo = async (id) => {\n    const { data } = await axios.get(TagNames.getVideoKey(id));\n    const videoLink = (await data.results[0]?.key)\n      ? TagNames.getVideoLink(data.results[0].key)\n      : \"\";\n\n    const video = videoLink ? true : false;\n    setVideoLink(videoLink);\n    setVideoStatus(video);\n  };\n\n  return (\n    <div className=\"MovieDetail\">\n      {movie.id && (\n        <div>\n          <img\n            className=\"back-cover\"\n            src={TagNames.getImagePath(movie.poster_path)}\n            srcSet={TagNames.getImagePath(movie.poster_path)}\n            alt={movie.title}\n            loading=\"lazy\"\n          ></img>\n          <div className=\"MovieInfo\">\n            <img\n              className=\"front-cover\"\n              src={TagNames.getImagePath(movie.backdrop_path)}\n              srcSet={TagNames.getImagePath(movie.backdrop_path)}\n              alt={movie.title}\n              loading=\"lazy\"\n            ></img>\n            <h1>{movie.title.toUpperCase()}</h1>\n            <div className=\"overview\">\n              <h5>Overview</h5>\n              <p>{movie.overview}</p>\n            </div>\n            <div>\n              <h4>Rating: {movie.vote_average}/10</h4>\n              <h4>Language: {movie.original_language.toUpperCase()}</h4>\n            </div>\n            {videoStatus && (\n              <div className=\"videoPlayer\">\n                <iframe\n                  src={videoLinK}\n                  title=\"YouTube video player\"\n                  frameborder=\"0\"\n                  allow=\"accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture\"\n                  allowfullscreen\n                ></iframe>\n              </div>\n            )}\n          </div>\n        </div>\n      )}\n    </div>\n  );\n};\n\nexport default MovieDetails;\n","/Users/santoshsingh/Documents/WebDevelopment/ReactJS/MoviesApp/src/components/pages/signup.jsx",[],"/Users/santoshsingh/Documents/WebDevelopment/ReactJS/MoviesApp/src/components/jsx/navbar/navbar.jsx",[],"/Users/santoshsingh/Documents/WebDevelopment/ReactJS/MoviesApp/src/components/js/constants/constants.js",[],"/Users/santoshsingh/Documents/WebDevelopment/ReactJS/MoviesApp/src/components/js/Notes.js",[],"/Users/santoshsingh/Documents/WebDevelopment/ReactJS/MoviesApp/src/components/jsx/moviesList.jsx",[],"/Users/santoshsingh/Documents/WebDevelopment/ReactJS/MoviesApp/src/components/jsx/form.jsx",["102","103"],"import React, { Component } from \"react\";\nimport Joi from \"joi-browser\";\nimport Input from \"./input\";\nimport Select from \"./select\";\nimport { Button } from \"@mui/material\";\n\n\nclass Form extends Component {\n  state = {\n    data: {},\n    errors: {},\n  };\n\n  schema = {\n    username: Joi.string().min(5).max(15).required(),\n    password: Joi.string().min(5).required(),\n  };\n\n  validate = () => {\n    const options = { abortEarly: false };\n    const { error } = Joi.validate(this.state.data, this.schema, options);\n\n    if (!error) return null;\n\n    const errors = {};\n    for (let item of error.details) errors[item.path[0]] = item.message;\n    return errors;\n  };\n\n  validateProperty = ({ name, value }) => {\n    const obj = { [name]: value };\n    const schema = { [name]: this.schema[name] };\n    const { error } = Joi.validate(obj, schema);\n    return error ? error.details[0].message : null;\n  };\n\n  handleSubmit = (e) => {\n    e.preventDefault();\n\n    const errors = this.validate();\n    this.setState({ errors: errors || {} });\n    if (errors) return;\n\n    this.doSubmit();\n  };\n\n  handleChange = ({ currentTarget: input }) => {\n    const errors = { ...this.state.errors };\n    const errorMessage = this.validateProperty(input);\n    if (errorMessage) errors[input.name] = errorMessage;\n    else delete errors[input.name];\n\n    const data = { ...this.state.data };\n    data[input.name] = input.value;\n\n    this.setState({ data, errors });\n  };\n\n  renderButton(label) {\n    return (\n      <Button variant=\"outlined\" size=\"normal\">\n        {label}\n      </Button>\n    );\n  }\n\n  renderSelect(name, label, options) {\n    const { data, errors } = this.state;\n\n    return (\n      <Select\n        name={name}\n        value={data[name]}\n        label={label}\n        options={options}\n        onChange={this.handleChange}\n        error={!!errors[name]}\n      />\n    );\n  }\n\n  renderInput(name, label, type = \"text\") {\n    const { data, errors } = this.state;\n\n    return (\n      <Input\n        type={type}\n        name={name}\n        value={data[name]}\n        label={label}\n        onChange={this.handleChange}\n        // helperText={errors[name]}\n        // error={!!errors[name]}\n      />\n    );\n  }\n\n  renderInputPassword(name, label, type = \"password\") {\n    const { data, errors } = this.state;\n\n 